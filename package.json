    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "web3": "^1.3.4"
  },
  "devDependencies": {
    "@types/react": "18.0.1",
    "@types/web3": "^1.2.2",
    "typescript": "4.5.5"
  },
  "license": "MIT"
}
