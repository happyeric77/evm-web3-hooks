{
  "name": "evm-web3-hooks",
  "version": "0.0.13",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "description": "EVM Web3 hook",
  "repository": {
    "type": "git",
    "url": "https://github.com/happyeric77/evm-web3-hooks.git"
  },
